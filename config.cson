"*":
  core:
    audioBeep: false
    telemetryConsent: "no"
  editor:
    fontFamily: "PT Mono"
    invisibles: {}
    showIndentGuide: true
    softWrapAtPreferredLineLength: true
  "exception-reporting":
    userId: "3a63a059-5290-7573-090f-92e00d5ec2ce"
  fonts:
    fontFamily: "Roboto Mono Light"
  "linter-ui-default":
    panelHeight: 96
  "prettier-atom":
    formatOnSaveOptions: {}
    prettierOptions:
      semi: false
      singleQuote: true
  tasks:
    archiveSeparator: "---"
    baseMarker: "[ ]"
    cancelledMarker: "[-]"
    completeMarker: "[x]"
  "tree-view":
    autoReveal: true
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
